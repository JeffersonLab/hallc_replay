      else TFile *F = new TFile(Form("../root_files/shms_calibration_%d.root", RunNumber));
      //Need to be more careful with file location
    }
  
  TH1F *hgc_e[4][4]; //[quadrant][pmt]
  TH1F *hgc_pi[4][4];
